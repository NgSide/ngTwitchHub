{
    "compileOnSave": false,
    "compilerOptions": {
      "outDir": "./dist/out-tsc",
      "baseUrl": "src",
      "sourceMap": true,
      "declaration": false,
      "moduleResolution": "node",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "noUnusedLocals": false,
      "noUnusedParameters": false,
      "noImplicitAny": false,
      "target": "es5",
      "paths": {
        "store": ["store.ts"]
      },
      "typeRoots": [
        "node_modules/@types"
      ],
      "lib": [
        "es2016",
        "dom"
      ],
      "types": [
        "jasmine"
      ]
    },
    "files": [
      "./src/main.ts",
      "./src/polyfills.ts"
    ],
    "include": [
    ]
  }
  